@InCollection{Borruso_(2004)_DensityAnalysisLarge,
  Title                    = {Density Analysis on Large Geographical Databases. Search for an Index of Centrality of Services at Urban Scale},
  Author                   = {Borruso, Giuseppe and Schoier, Gabriella},
