@Article{Maantay_(2002)_MappingEnvironmentalInjustices:,
  Title                    = {Mapping Environmental Injustices: Pitfalls and Potential of Geographic Information Systems in Assessing Environmental Health and Equity},
  Author                   = {Maantay, Juliana},
  Journal                  = {Environmental health perspectives},
  Year                     = {2002},

  Month                    = {apr},
  Note                     = {Geographic Information Systems ({GIS)} have been used increasingly to map instances of environmental injustice, the disproportionate exposure of certain populations to environmental hazards. Some of the technical and analytic difficulties of mapping environmental injustice are outlined in this article, along with suggestions for using {GIS} to better assess and predict environmental health and equity. I examine 13 {GIS-based} environmental equity studies conducted within the past decade and use a study of noxious land use locations in the Bronx, New York, to illustrate and evaluate the differences in two common methods of determining exposure extent and the characteristics of proximate populations. Unresolved issues in mapping environmental equity and health include lack of comprehensive hazards databases; the inadequacy of current exposure indices; the need to develop realistic methodologies for determining the geographic extent of exposure and the characteristics of the affected populations; and the paucity and insufficiency of health assessment data. {GIS} have great potential to help us understand the spatial relationship between pollution and health. Refinements in exposure indices; the use of dispersion modeling and advanced proximity analysis; the application of neighborhood-scale analysis; and the consideration of other factors such as zoning and planning policies will enable more conclusive findings. The environmental equity studies reviewed in this article found a disproportionate environmental burden based on race and/or income. It is critical now to demonstrate correspondence between environmental burdens and adverse health impacts-to show the disproportionate effects of pollution rather than just the disproportionate distribution of pollution sources.},
  Pages                    = {161--171},
  Volume                   = {110},

  ISSN                     = {00916765},
  Shorttitle               = {Mapping Environmental Injustices},
  Url                      = {http://www.jstor.org/stable/3455050},
  Urldate                  = {2008-07-27}
}

