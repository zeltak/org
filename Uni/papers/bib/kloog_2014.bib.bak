@Article{Korkmaz_(2008)_Rolemelatoninin,
  Title                    = {Role of melatonin in the epigenetic regulation of breast cancer},
  Author                   = {Korkmaz, Ahmet and Sanchez-Barcelo, Emilio and Tan, Dun-Xian and Reiter, Russel},
  Journal                  = {Breast Cancer Research and Treatment},
  Year                     = {2008},

  Month                    = {jul},
  Note                     = {{PMID:} 18592373},

  Abstract                 = {The oncostatic properties of melatonin as they directly or indirectly involve epigenetic mechanisms of cancer are reviewed with a special focus on breast cancer. Five lines of evidence suggest that melatonin works via epigenetic processes: (1) melatonin influences transcriptional activity of nuclear receptors ({ERalpha}, {GR} and {RAR)} involved in the regulation of breast cancer cell growth; (2) melatonin down-regulates the expression of genes responsible for the local synthesis or activation of estrogens including aromatase, an effect which may be mediated by methylation of the {CYP19} gene or deacetylation of {CYP19} histones; (3) melatonin inhibits telomerase activity and expression induced by either natural estrogens or xenoestrogens; (4) melatonin modulates the cell cycle through the inhibition of cyclin D1 expression; (5) melatonin influences circadian rhythm disturbances dependent on alterations of the light/dark cycle (i.e., light at night) with the subsequent deregulation of {PER2} which acts as a tumor suppressor gene.},
  Doi                      = {10.1007/s10549-008-0103-5},
  ISSN                     = {0167-6806},
  Url                      = {http://www.ncbi.nlm.nih.gov/pubmed/18592373},
  Urldate                  = {2008-08-06}
}

